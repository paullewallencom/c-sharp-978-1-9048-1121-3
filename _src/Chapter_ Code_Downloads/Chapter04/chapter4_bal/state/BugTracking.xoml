			<SetStateActivity x:Name="setStateActivity5" TargetStateName="ClosedState" />
		</EventDrivenActivity>
	</StateActivity>
	<StateActivity x:Name="DeferredSate">
		<EventDrivenActivity x:Name="assignEvent2">
