<SequentialWorkflowActivity x:Class="chapter4_bal.ws.WebServices" x:Name="WebServices" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/workflow">
